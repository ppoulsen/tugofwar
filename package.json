{
  "name": "tugofwar",
  "version": "1.0.0",
  "description": "Scripts to run server and client simultaneously",
  "main": "index.js",
  "repository": "https://github.com/ppoulsen/tugofwar.git",
  "scripts": {
    "client:dev": "cd client && npm run start",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && npm run watch"
  },
  "author": "Paul Poulsen <ppoulsen21@gmail.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
